#define _POSIX_C_SOURCE 200809L
#include <stdio.h>
#include <stdlib.h>
#include <string.h>
#include <unistd.h>

int main() {
  char *buff = NULL;
  size_t len = 0;
  printf("Enter programs to run.\n");
  while (buff != "exit\n") {
    getline(&buff, &len, stdin);
    execl(buff, (char *)NULL);
  }
}
